{
  "mcpServers": {
    "sequential-thinking": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "ide-assistant",
        "--enable-web-dashboard",
        "False",
        "--project",
        "%cd%"
      ],
      "env": {}
    },
    "rust-lsp": {
      "type": "stdio",
      "command": "bun",
      "args": [
        "x",
        "@mizchi/lsmcp",
        "--bin=rust-analyzer"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "claude-context": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@zilliz/claude-context-mcp@latest"
      ],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "MILVUS_TOKEN": "4b524b55a9b425b040d9337ae17c0c8cd7acfa6965d1c58b00ef59da89100a34c0e7717977b69f829f9fe5aeeabd85b3c8dfacaf"
      }
    },
    "docs-rs": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@nuskey8/docs-rc-mcp"
      ],
      "env": {}
    }
  }
}