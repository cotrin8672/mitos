{
  "mcpServers": {
    "sequential-thinking": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--enable-web-dashboard",
        "False",
        "--project",
        "%cd%"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "claude-context": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@zilliz/claude-context-mcp@latest"
      ],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "MILVUS_TOKEN": "${MILVUS_TOKEN}"
      }
    },
    "docs-rs": {
      "type": "stdio",
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@nuskey8/docs-rs-mcp"
      ],
      "env": {}
    }
  }
}